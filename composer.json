{
    "name": "mischasigtermans/laravel-ogkit",
    "description": "Dynamic OG images for Laravel. Design social previews with your own HTML and CSS.",
    "keywords": ["laravel", "og-image", "open-graph", "social-preview", "seo"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mischa Sigtermans",
            "email": "mischa@sigtermans.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.0|^3.0",
        "laravel/pint": "^1.20",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Ogkit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ogkit\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ogkit\\OgkitServiceProvider"
            ],
            "aliases": {
                "Ogkit": "Ogkit\\Facades\\Ogkit"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}